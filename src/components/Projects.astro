---
import { RESUME } from '../../components/Work-Experience/resume-data.js';

const { projects } = RESUME;
---

<div class="mt-8 pl-10">
  <h2 class="font-serif text-4xl border-b mb-2">Projects</h2>

  {
    projects.map((project) => (
      <div class="mt-4">
        <h4 class="font-bold text-sm">{project.name}</h4>
        <p class="text-sm text-gray-700 mt-1 leading-relaxed">{project.description}</p>
      </div>
    ))
  }
</div>
